email}\n                        onChange={e => onChange(e)}\n                        required />\n                  </Form.Group>\n                  <Form.Group className=\"d-flex search-field\">\n                    <Form.Control type=\"password\" placeholder=\"Password\" size=\"lg\" className=\"h-auto\"  name='password'\n                        value={password}\n                        onChange={e => onChange(e)}\n                        minLength='6'\n                        required/>\n                  </Form.Group>\n                  <div className=\"mt-3\">\n                    <button className=\"btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn\" type='submit'>SIGN IN</button>\n                  </div>\n                  \n                 \n                  \n                </Form>\n                <div className=\"mb-2\">\n                    <button type=\"button\" className=\"btn btn-block btn-gmail auth-form-btn\" onClick={continueWithGoogle}>\n                      <i className=\"mdi mdi-gmail mr-2\"></i>Connect using Gmail\n                    </button>\n                  </div>\n              </div>\n              \n            </div>\n          </div>\n        </div>  \n      </div>\n    )\n  \n}\n\nconst mapStateToProps = state => ({\n  isAuthenticated: state.auth.isAuthenticated\n});\n\nexport default connect(mapStateToProps, { login })(Login);\n"]}}